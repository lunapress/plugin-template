{
    "name": "frontend",
    "packageManager": "pnpm@10.19.0",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "format": "prettier --write .",
        "lint:typescript": "tsc --noEmit -p tsconfig.app.json",
        "lint:eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
        "lint:prettier": "prettier --check ./src",
        "lint": "npm-run-all lint:*",
        "lint-parallel": "run-p lint:*"
    },
    "dependencies": {},
    "devDependencies": {
        "@eslint-react/eslint-plugin": "2.2.3",
        "@eslint/js": "9.38.0",
        "@types/jquery": "3.5.33",
        "@types/node": "24.9.1",
        "@types/react": "19.2.2",
        "@types/react-dom": "19.2.2",
        "@vitejs/plugin-react-swc": "4.2.0",
        "eslint": "9.38.0",
        "eslint-plugin-react-hooks": "7.0.1",
        "eslint-plugin-react-refresh": "0.4.24",
        "globals": "16.4.0",
        "moment": "2.30.1",
        "npm-run-all2": "8.0.4",
        "prettier": "3.6.2",
        "rollup-plugin-external-globals": "0.13.0",
        "typescript": "5.9.3",
        "typescript-eslint": "8.46.2",
        "vite": "7.1.12",
        "@lunapress/rollup-plugin-wp": "dev"
    },
    "pnpm": {
        "supportedArchitectures": {
            "os": [
                "linux",
                "current"
            ],
            "cpu": [
                "x64",
                "arm64"
            ]
        }
    }
}